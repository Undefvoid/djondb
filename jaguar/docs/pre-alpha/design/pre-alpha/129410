        if (action->compare(\"NEW\") == 0) {
            if (log->isDebug()) log->debug(\"Request NEW received.\");
            int procDef = atoi(request->getParameter(\"DEFI\")->c_str());
            if (log->isDebug()) log->debug(\"process id: \" + toString(procDef));

