package com.sinco.persistence;

/**
 *
 * @author cross
 */
@Entity
public class InheritedEntity extends com.sinco.persistence.ParentEntity {

    private int id;
    private String name;

    @Id
    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
    
}
